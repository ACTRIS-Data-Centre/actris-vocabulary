ACTRIS_ctrl_lists:checkonlidarratio a skos:Concept;
  skos:prefLabel "check on lidar ratio"@en;
  skos:definition "This control performs a check on the Lidar Ratio values when Extinction and Backscatter are provided in the same product file (i.e. are provided at the same vertical resolution). Lidarratio is defined as positive value and values are typically between 10 and 120 sr."@en;
  skos:broader ACTRIS_ctrl_lists:boundarycheck;
  skos:related ACTRIS_vocab:aerosolremotesensing;
  dct:creator <https://orcid.org/0000-0001-8301-1319>;
  dct:contributor <https://orcid.org/0000-0003-4157-0838>;
  skos:inScheme <https://vocabulary.actris.nilu.no/actris_controlled_lists/> .

ACTRIS_ctrl_lists:checkonvolumedepolarization a skos:Concept;
  skos:prefLabel "check on volume depolarization"@en;
  skos:definition "This control is performed on the volume depolarization variable and its error (error_volumedepolarization) in order to check that the following conditions are preserved : |volumedepolarization|< 3 error_volumedepolarization volumedepolarization must be between [0, 1] within its error"@en;
  skos:broader ACTRIS_ctrl_lists:boundarycheck;
  skos:related ACTRIS_vocab:aerosolremotesensing;
  dct:creator <https://orcid.org/0000-0001-8301-1319>;
